{
  "name": "ncjoes/php-poppler",
  "description": "PHP wrapper for Poppler utils",
  "keywords": ["PDF Convert", "PHP-PDF", "PDF to HTML", "PDF to SVG", "PDF to JPG", "PDF to PNG", "Poppler-utils"],
  "license": "MIT",
  "authors": [
    {
      "name": "Chukwuemeka Nwobodo",
      "email": "jc.nwobodo@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.*",
    "illuminate/config": "5.3.*",
    "illuminate/support": "5.3.*"
  },
  "require-dev":{
    "phpunit/phpunit":"5.5.*"
  },

  "autoload": {
    "psr-0": {
      "NcJoes": "src"
    },
    "psr-4": {
      "NcJoes\\PhpPoppler\\": "src/"
    }
  }
}
